//https://developer.chrome.com/extensions/getstarted
{
  "name":"myDictionary",
  "version":"1.0",
  "manifest_version": 2,
  "description":"A personalized dictionary app",
  "icons":{
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://www.gstatic.com https://*.firebaseio.com; object-src 'self'"


}
